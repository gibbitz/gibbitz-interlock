<label class=" {{#if class}}{{class}}{{/if}}">
  <input
    name="{{name}}"
    {{checked value}}
    type="checkbox"
    {{disabled disabled}}
    data-selector="{{selector}}"
    data-uuid="{{uuid}}"
    data-key="{{key}}"
    data-render="{{render}}"
  />
  <div class="label">{{localize label}}</div>
</label>