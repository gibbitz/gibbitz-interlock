{{systemLog ' PROGRAMS PARTIAL | ' this}}
{{#if this.length}}
  {{#cp2020-sectionBox title='Programs' class='item-list__wrapper'}}
    <ul class="item-list item-list--programs">
      {{#each this}}
        <li
          class="item item--programs"
          data-item-id="{{_id}}"
          data-item-name="{{this.name}}"
        >
          <a
            class="item--label"
            data-selector="item-edit"
            data-uuid="{{_id}}"
            title="edit"
          >
            {{this.name}}
          </a>
          {{cp2020-button
            class="program__roll"
            uuid=_id
            roll=true
            selector='item-roll'
            label='execute'
          }}
        </li>
      {{/each}}
    </ul>
  {{/cp2020-sectionBox}}
{{/if }}
