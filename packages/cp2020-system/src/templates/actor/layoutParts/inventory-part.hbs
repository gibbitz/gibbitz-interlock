<h3>Upgrades</h3>
<ul class="item-list">
  {{#each actor.itemTypes.Upgrade}}
  {{outfit this}}
  {{/each}}
</ul>

<h3>Weapons</h3>
<ul class="item-list">
  {{#each actor.itemTypes.Weapon}}
  {{weapon this}}
  {{/each}}
</ul>

<ul class="item-list">
  {{#each actor.itemTypes.Outfit}}
  {{outfit this}}
  {{/each}}
</ul>

<ul class="item-list">
  {{#each actor.itemTypes.Vehicle}}
  {{vehicle this}}
  {{/each}}
</ul>